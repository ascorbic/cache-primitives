{
  "name": "cache-handlers",
  "version": "0.0.0",
  "description": "Modern CDN cache primitives using web-standard middleware",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {

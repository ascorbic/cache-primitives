    "undici": "^7.13.0",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ascorbic/cdn-cache-control.git",
    "directory": "packages/cache-handlers"
  },
  "homepage": "https://github.com/ascorbic/cdn-cache-control",
